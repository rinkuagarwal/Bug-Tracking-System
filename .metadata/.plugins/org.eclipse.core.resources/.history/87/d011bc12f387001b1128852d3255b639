import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

import junit.framework.Assert;

class TestBug {

	@Test
	void testFeedback() {
		Manager managers=new Manager(null);
		String result=managers.feedback();
		Assert.assertEquals("bug solved",result);
		
		
	}
	@Test
	void testAssignBug() {
		Manager managers=new Manager(null);
		String result=managers.assignBug();
		Assert.assertEquals("bug to be alloted",result);
		
		
	}


}
